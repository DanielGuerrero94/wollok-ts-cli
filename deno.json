{
  "name": "wollok-ts-cli",
  "version": "0.2.11",
  "description": "Wollok Command Line Interface",
  "repository": "https://github.com/uqbar-project/wollok-ts-cli",
  "license": "MIT",
  "author": {
    "name": "Uqbar Foundation",
    "email": "org.uqbar@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/uqbar-project/wollok-ts-cli/issues"
  },
  "homepage": "https://github.com/uqbar-project/wollok-language",
  "keywords": [
    "wollok",
    "cli",
    "typescript"
  ],
  "exports": "./src/index.ts",
  "publish": {
    "include": [
      "src/",
      "README.md",
      "LICENSE",
      "deno.json"
    ]
  },
  "tasks": {
    "start": "deno run ./src/index.ts",
    "prepare": "node ./scripts/download-libs.js && npm run build:tools",
    "test": "npm run lint && npm run test:unit",
    "test:diagram": "mocha -r ts-node/register/transpile-only test/**/diagram.test.ts --timeout 5000",
    "test:run": "mocha -r ts-node/register/transpile-only test/**/run.test.ts --timeout 2000",
    "test-with-coverage": "npm run lint && nyc --reporter=json --lines 70 npm run test:unit",
    "test:unit": "mocha --parallel -r ts-node/register/transpile-only test/**/*.test.ts --timeout 5000",
    "watch": "deno run --watch ./src/index.ts",
    "pack": "pkg .",
    "prepublishOnly": "npm run prepare && npm run build && npm run test"
  },
  "lint": {
    "include": ["src/"],
    "rules": {
      "exclude": ["no-explicit-any", "verbatim-module-syntax"]
    }
  },
  "fmt": {
    "include": ["src/"]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "chalk": "npm:chalk@^5.3.0",
    "commander": "npm:commander@^11.1.0",
    "cors": "npm:cors@^2.8.5",
    "cytoscape": "npm:cytoscape@^3.15.2",
    "express": "npm:express@^4.18.1",
    "globby": "npm:globby@^11.0.4",
    "loglevel": "npm:loglevel@^1.8.1",
    "p5": "npm:p5@^1.8.0",
    "pkg": "npm:pkg@^5.8.1",
    "socket.io": "npm:socket.io@^4.5.1",
    "winston": "npm:winston@^3.11.0",
    "wollok-ts": "npm:wollok-ts@^4.1.8",
    "wollok-web-tools": "npm:wollok-web-tools@^1.1.7",
    "@types/chai": "npm:@types/chai@^4.3.16",
    "@types/chai-as-promised": "npm:@types/chai-as-promised@^7.1.7",
    "@types/cors": "npm:@types/cors@^2.8.17",
    "@types/cytoscape": "npm:@types/cytoscape@^3.19.7",
    "@types/express": "npm:@types/express@^4.17.20",
    "@types/mocha": "npm:@types/mocha@^10.0.7",
    "@types/p5": "npm:@types/p5@^1.7.1",
    "@types/sinon": "npm:@types/sinon@^17.0.0",
    "chai": "npm:chai@^4.5.0",
    "chai-as-promised": "npm:chai-as-promised@^7.1.1",
    "chai-http": "npm:chai-http@^4.4.0",
    "mocha": "npm:mocha@^10.7.0",
    "nyc": "npm:nyc@^15.1.0",
    "shx": "npm:shx@^0.3.4",
    "sinon": "npm:sinon@^17.0.1",
    "socket.io-client": "npm:socket.io-client@^4.7.5"
  }
}
